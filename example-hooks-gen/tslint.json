{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-plugin-relay"],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "variable-name": false,
    "trailing-comma": false,
    "quotemark": false,
    "ordered-imports": false,
    "no-console": false,
    "semicolon": false,
    "member-access": false,
    "curly": false,
    "object-literal-shorthand": false
  },
  "rulesDirectory": []
}
